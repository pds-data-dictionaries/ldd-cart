        Vector_Projection_Z_Axis. </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="cart:projection_elevation_WO_Units">
        <xs:attribute name="unit" type="pds:Units_of_Angle" use="required" />
      </xs:extension>
    </xs:simpleContent>
